pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 menu = true
 sources = {'worm','hamster','chicken','ferret','r.master'}
 computers = {'pc server','quadcore','rpi cluster','pc cluster','c= 64 cluster'}
 machines = {}
 for i=1,4,1 do
  machines[i] = def_powersource(i)
 end
 x1 = {16,80,16,80}
 y1 = {72,72,112,112}
 tick = 0
 pov = def_pov()
 moneez = 100
 icanhaz = 1
end

function _draw()
 cls(1)
 if menu then
  draw_menu()
 else
  draw_screen()
 end
end

function _update()
 if menu then
  update_menu()
 else
  game_logic()
  tick = tick + 1
 end
end

-- menu stuff
function draw_menu()
 color(8)
 print "the internet vs ludum dare"
 color(9)
 print "the launch power struggle"
 color(15)
 print "a ld39 game"
 color(10)
 print "inspired by every ld launch"
 color(3)
 print "by team (\""
 print("\x97 to start!", 30, 40, 5)
end

function update_menu()
 if btn(5) then
  menu = false
 end
end

-- draw stuff

function draw_screen()
 draw_status()
 foreach(machines,draw_machine)
 draw_pov()
end

function draw_machine(m)
 local t = animn(4,3)
 local x=x1[m.num]
 local y=y1[m.num]
 local a = {7,23,39,55}
 local v = {43,16,32,48,59}
 circ(x+19,y-11,10,6)
 circ(x+18,y-12,10,7)
 rect(x-2,y-2,x+38,y+12,6)
 rectfill(x-1,y-1,x+39,y+13,13)
 print(sources[m.name],x,y,15)
 spr(pmeter(m.percent),x,y+6,1,1)
 local b = pmeter(m.tired)
 spr(a[b]+t,x+15,y+6,1,1)
 spr(10+pmeter(m.hunger),x+30,y+6,1,1)
 spr(27+animn(2,20),x,y-9)
 spr(v[m.name]+t,x+15,y-10)
end

function pmeter(p)
  return flr((p*100)/25)
end

function draw_pov()
  spr(5,pov.x,pov.y,2,2,pov.facing)
end

function animn(n,d)
  local x = flr(tick % (n*d))
  x = flr(x/d)
  return x
end

function draw_status()
  rectfill(1,1,127,48,5)
  for i=31,48,2 do
   line(1,i,125,i,1)
   line(1,i+1,125,i+1,0)
  end
  rect(0,0,126,47,6)
  spr(64,0,0,16,4)
  clock()
  isup('servers are ',false, 2, 32,1)
  isup('irc is ',true,74,32,1)
  isup('people are ',false, 2, 40,2)
  print(computers[icanhaz],75,41,2)
  print(computers[icanhaz],74,40,7)
  print("\x92",101,7,8)
  print("\x92",100,8,10)
  print(moneez,111,7,8)
  print(moneez,110,8,10)
end

function clock()
  local t1 = flr(tick/20)
  local m1 = flr(t1/60)
  local s1 = t1 % 60
  local s2 = s1 % 10
  s1 = flr((s1-s2)/10)

  print(t1,0,0,0)
  print(m1,20,0,0)
  print(s1,0,8,0)
  print(s2,8,8,0)

  local s1 = t1 - 60*m1
  local s2 = s1 % 10
  s1 = flr(s1 / 10)
  spr(128,44,2,1,3)
  spr(128+(m1),44+8,2,1,3 )
  spr(138+animn(2,15),58,2,1,3)
  spr(128+s1,48+8+8,2,1,3 )
  spr(128+s2,48+8+16,2,1,3 )
end

function isup(msg, up, x,y,gr)
  print(msg,x,y,7)
  txt = { { 'up!','down!' }, {'happy!','angry!'} }
  if (up) then
    print(txt[gr][1],#msg*4+x,y,3)
  else
    print(txt[gr][2],#msg*4+x,y,8)
  end
end

-- game stuff

function def_powersource(n)
 source = {}
 source.num = n
 source.name = 1
 source.percent = 0.5
 source.tired = 0.5
 source.hunger = 0.5
 return source
end

function def_pov()
 pov = {}
 pov.x = 36
 pov.y = 80
 pov.facing = false
 pov.holding = 0
 return pov
end

function joydir()
  local r = ((1 - intbool(btn(0))) + intbool(btn(1)) ) - 1
  local s = ((1 - intbool(btn(2))) + intbool(btn(3)) ) - 1
  return {r,s}
end

function intbool(i)
  if (i) then return 1 else return 0 end
end

function movpov()
  local x = joydir()
  pov.x += x[1]
  pov.y += x[2]
  pov.facing = x[1] == 1
end

function game_logic()
  movpov()
end

